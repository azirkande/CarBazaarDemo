@model CarSalesDemo.Web.ViewModels.CarListingViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div class="carListing">

    @if (Model.Cars != null && Model.Cars.Count > 0)
    {

        @foreach (var car in Model.Cars)
        {
            { Html.RenderPartial("_CarListing", car); }
        }

    }
    else
    {
        <h3>No cars to display.</h3>
    }

</div>